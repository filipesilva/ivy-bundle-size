{
  "name": "cli-hello-world-ivy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "bazel": "bazel",
    "bazel-current": "bazel build //src:bundle_current",
    "cli-current": "ng build --prod",
    "cli-ivy": "ng build --prod --tsConfig=src/tsconfig.ngtsc.json --outputPath=dist/cli-ivy",
    "ngc": "ngc -p src/tsconfig.ngc.json",
    "ngtsc": "ngc -p src/tsconfig.ngtsc.json",
    "rollup-current": "rollup -c src/rollup.config.js",
    "rollup-ivy": "rollup -c src/rollup.config.js -i ./out-tsc/tsc-ivy/main.ngtsc.js -o ./dist/rollup-ivy/main.js",
    "ngcc": "ivy-ngcc -p src/tsconfig.ngtsc.json",
    "current": "yarn cli-current && yarn ngc && yarn rollup-current && yarn bazel-current",
    "ivy": "yarn ngcc && yarn cli-ivy && yarn ngtsc && yarn rollup-ivy"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@angular/router": "7.2.0",
    "rxjs": "6.3.3",
    "tslib": "1.9.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/bazel": "7.2.0",
    "@angular-devkit/build-angular": "0.12.1",
    "@angular-devkit/build-optimizer": "0.12.1",
    "@angular/cli": "7.2.1",
    "@angular/compiler-cli": "7.2.0",
    "@bazel/bazel": "0.21.0",
    "@bazel/typescript": "0.22.0",
    "rollup": "1.1.0",
    "rollup-plugin-filesize": "6.0.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-terser": "4.0.2",
    "typescript": "3.2.2"
  }
}
