{
  "name": "cli-hello-world-ivy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cli-current": "ng build --prod",
    "cli-ivy": "ng build --prod --tsConfig=src/tsconfig.app.ngtsc.json --outputPath=dist/cli-ivy",
    "ngc": "ngc -p src/tsconfig.app.json",
    "ngtsc": "ngc -p src/tsconfig.app.ngtsc.json",
    "rollup-current": "rollup -c src/rollup.config.js",
    "rollup-ivy": "rollup -c src/rollup.config.js -i ./out-tsc/tsc-ivy/main.ngtsc.js -o ./dist/rollup-ivy/main.js",
    "ngcc": "ivy-ngcc -p src/tsconfig.app.ngtsc.json",
    "current": "npm run cli-current && npm run ngc && npm run rollup-current",
    "ivy": "npm run ngcc && npm run cli-ivy && npm run ngtsc && npm run rollup-ivy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.0",
