{
  "name": "cli-hello-world-ivy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "bazel": "bazel",
    "//build-scripts": "",
    "build": "yarn clean-ngcc && yarn current && yarn ngcc && yarn ivy ",
    "bazel-current": "bazel build //src:bundle_current",
    "bazel-ivy": "bazel build //src:bundle_ivy",
    "cli-current": "ng build --prod",
    "cli-ivy": "ng build --prod --tsConfig=src/tsconfig.ngtsc.json --outputPath=dist/cli-ivy --main=src/main.ngtsc.ts",
    "ngc": "ngc -p src/tsconfig.ngc.json",
    "ngtsc": "ngc -p src/tsconfig.ngtsc.json",
    "rollup-current": "rollup -c src/rollup.config.js",
    "rollup-ivy": "rollup -c src/rollup.config.js -i ./out-tsc/tsc-ivy/main.ngtsc.js -o ./dist/rollup-ivy/main.js",
    "ngcc": "ivy-ngcc -p src/tsconfig.ngtsc.json",
    "current": "yarn cli-current && yarn ngc && yarn rollup-current && yarn bazel-current",
    "ivy": "yarn cli-ivy && yarn ngtsc && yarn rollup-ivy && yarn bazel-ivy",
    "clean-ngcc": "rm -rf node_modules/@angular && yarn install --check-files",
    "//test-scripts": "",
    "postinstall": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.45",
    "update-windows-snapshots": "yarn update-windows-cli-rollup-snapshots && yarn update-windows-bazel-current-snapshots && yarn update-windows-bazel-ivy-snapshots",
    "update-linux-snapshots": "yarn update-linux-cli-rollup-snapshots && yarn update-linux-bazel-current-snapshots && yarn update-linux-bazel-ivy-snapshots",
    "update-windows-cli-rollup-snapshots": "cp -r dist/* snapshots/windows/",
    "update-linux-cli-rollup-snapshots": "cp -r dist/* snapshots/linux/",
    "update-windows-bazel-current-snapshots": "rm -f snapshots/windows/bazel-current/*.min.* && cp ./bazel-out/x64_windows-fastbuild/bin/src/bundle_current.min.* snapshots/windows/bazel-current/",
    "update-windows-bazel-ivy-snapshots": "rm -f snapshots/windows/bazel-ivy/*.min.* && cp ./bazel-out/x64_windows-fastbuild/bin/src/bundle_ivy.min.* snapshots/windows/bazel-ivy/",
    "update-linux-bazel-current-snapshots": "rm -f snapshots/linux/bazel-current/*.min.* && cp ./dist-bazel/bin/src/bundle_current.min.* snapshots/linux/bazel-current/",
    "update-linux-bazel-ivy-snapshots": "rm -f snapshots/linux/bazel-ivy/*.min.* && cp ./dist-bazel/bin/src/bundle_ivy.min.* snapshots/linux/bazel-ivy/",
    "serve": "http-server ./snapshots",
    "test": "concurrently -s=first \"yarn serve -s\" \"protractor ./e2e/protractor.conf.js\" --kill-others"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@angular/router": "7.2.0",
    "rxjs": "6.3.3",
    "tslib": "1.9.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.12.1",
    "@angular-devkit/build-optimizer": "0.12.1",
    "@angular/bazel": "7.2.0",
    "@angular/cli": "7.2.1",
    "@angular/compiler-cli": "7.2.0",
    "@bazel/bazel": "0.21.0",
    "@bazel/typescript": "0.22.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "concurrently": "4.1.0",
    "http-server": "0.11.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "protractor": "5.4.2",
    "puppeteer": "1.11.0",
    "rollup": "1.1.0",
    "rollup-plugin-filesize": "6.0.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-terser": "4.0.2",
    "ts-node": "7.0.1",
    "typescript": "3.2.2"
  }
}
