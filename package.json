{
  "name": "cli-hello-world-ivy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "bazel": "bazel",
    "//build-scripts": "",
    "build": "yarn clean-ngcc && yarn current && yarn ngcc && yarn ivy && yarn ivy-compat",
    "bazel-current": "bazel build //src:bundle_current",
    "bazel-ivy": "bazel build --define=compile=aot //src:bundle_ivy",
    "bazel-ivy-compat": "bazel build --define=compile=aot //src:bundle_ivy_compat",
    "cli-current": "ng build --prod",
    "cli-ivy": "ng build --prod --tsConfig=src/tsconfig.ivy.json --outputPath=dist/cli-ivy --main=src/main.ivy.ts",
    "cli-ivy-compat": "ng build --prod --tsConfig=src/tsconfig.ivy.json --outputPath=dist/cli-ivy-compat",
    "ngc": "ngc -p src/tsconfig.current.json",
    "ngtsc": "ngc -p src/tsconfig.ivy.json",
    "rollup-current": "rollup -c src/rollup.config.js",
    "rollup-ivy": "rollup -c src/rollup.config.js -i ./out-tsc/tsc-ivy/main.ivy.js -o ./dist/rollup-ivy/main.js",
    "rollup-ivy-compat": "rollup -c src/rollup.config.js -i ./out-tsc/tsc-ivy/main.ivy-compat.js -o ./dist/rollup-ivy-compat/main.js",
    "ngcc": "ivy-ngcc",
    "current": "yarn cli-current && yarn ngc && yarn rollup-current && yarn bazel-current",
    "ivy": "yarn cli-ivy && yarn ngtsc && yarn rollup-ivy && yarn bazel-ivy",
    "ivy-compat": "yarn cli-ivy-compat && yarn ngtsc && yarn rollup-ivy-compat && yarn bazel-ivy-compat",
    "clean-ngcc": "rm -rf node_modules/@angular && yarn install --check-files",
    "clean-bazel-symlinks": "rm -rf bazel-out && rm -rf dist-bazel",
    "//test-scripts": "",
    "postinstall": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.45",
    "update-windows-snapshots": "yarn update-windows-cli-rollup-snapshots && yarn update-windows-bazel-current-snapshots && yarn update-windows-bazel-ivy-snapshots && update-windows-bazel-ivy-compat-snapshots",
    "update-linux-snapshots": "yarn update-linux-cli-rollup-snapshots && yarn update-linux-bazel-current-snapshots && yarn update-linux-bazel-ivy-snapshots && update-linux-bazel-ivy-compat-snapshots",
    "update-windows-cli-rollup-snapshots": "cp -r dist/* snapshots/windows/",
    "update-linux-cli-rollup-snapshots": "cp -r dist/* snapshots/linux/",
    "update-windows-bazel-current-snapshots": "rm -f snapshots/windows/bazel-current/*.min.* && cp ./bazel-out/x64_windows-fastbuild/bin/src/bundle_current.min.* snapshots/windows/bazel-current/",
    "update-windows-bazel-ivy-snapshots": "rm -f snapshots/windows/bazel-ivy/*.min.* && cp ./bazel-out/x64_windows-fastbuild/bin/src/bundle_ivy.min.* snapshots/windows/bazel-ivy/",
    "update-windows-bazel-ivy-compat-snapshots": "rm -f snapshots/windows/bazel-ivy-compat/*.min.* && cp ./bazel-out/x64_windows-fastbuild/bin/src/bundle_ivy_compat.min.* snapshots/windows/bazel-ivy-compat/",
    "update-linux-bazel-current-snapshots": "rm -f snapshots/linux/bazel-current/*.min.* && cp ./dist-bazel/bin/src/bundle_current.min.* snapshots/linux/bazel-current/",
    "update-linux-bazel-ivy-snapshots": "rm -f snapshots/linux/bazel-ivy/*.min.* && cp ./dist-bazel/bin/src/bundle_ivy.min.* snapshots/linux/bazel-ivy/",
    "update-linux-bazel-ivy-compat-snapshots": "rm -f snapshots/linux/bazel-ivy-compat/*.min.* && cp ./dist-bazel/bin/src/bundle_ivy_compat.min.* snapshots/linux/bazel-ivy-compat/",
    "serve": "http-server ./snapshots",
    "test": "concurrently -s=first \"yarn serve -s\" \"protractor ./e2e/protractor.conf.js\" --kill-others",
    "sme": "source-map-explorer"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "github:angular/common-builds#8.0.0-beta.10+a6809e0",
    "@angular/compiler": "github:angular/compiler-builds#8.0.0-beta.10+a6809e0",
    "@angular/core": "github:angular/core-builds#8.0.0-beta.10+a6809e0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#8.0.0-beta.10+a6809e0",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#8.0.0-beta.10+a6809e0",
    "@angular/router": "github:angular/router-builds#8.0.0-beta.10+a6809e0",
    "rxjs": "6.3.3",
    "tslib": "1.9.0",
    "zone.js": "0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.0-beta.10",
    "@angular-devkit/build-optimizer": "0.800.0-beta.10",
    "@angular/bazel": "7.2.0",
    "@angular/cli": "7.2.1",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#8.0.0-beta.10+a6809e0",
    "@bazel/bazel": "0.21.0",
    "@bazel/typescript": "0.22.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "concurrently": "4.1.0",
    "http-server": "0.11.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "protractor": "5.4.2",
    "puppeteer": "1.11.0",
    "rollup": "1.1.0",
    "rollup-plugin-filesize": "6.0.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-terser": "4.0.2",
    "source-map-explorer": "^1.6.0",
    "ts-node": "7.0.1",
    "typescript": "3.3.4000"
  }
}
